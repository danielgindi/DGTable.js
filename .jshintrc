{
/* true enables warnings */
	"bitwise": true,
	"camelcase": true,
	"curly": false,
	"eqeqeq": false,
	"esversion": 5,
	"forin": true,
	"immed": true,
	"indent": 4,
	"latedef": false,
	"newcap": true,
	"noarg": true,
	"noempty": true,
	"nonbsp": true,
	"nonew": true,
	"plusplus": false,
	"quotmark": "single",
	"undef": true,
	"unused": false, /* JSDoc typedefs for closure compiler are unused... */ 
	"trailing": true,
	
/* true produces less warnings */
	"boss": false,
	"eqnull": true,
	"expr": true,
	"smarttabs": true,
	"browser": true,
	"proto": true,
	"sub": true,
	"shadow": true
}